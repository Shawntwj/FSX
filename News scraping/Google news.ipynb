    "# prevent error429 by working with batches (per month)\n",
    "time_period = {1: ['01/01/2021', '01/31/2021'],\n",
    "              2: ['02/01/2021', '02/28/2021'],\n",
    "              3: ['03/01/2021', '03/31/2021'],\n",
