    "toSearch = 'food supply'\n",
    "def getGoogleNews(terms, start, end):\n",
    "    parsed_news = getGoogleNewsLinks(terms, start, end)\n",
    "    news_df = getArticleSummary(parsed_news)\n",
    "    return(news_df)\n",
    "\n"
